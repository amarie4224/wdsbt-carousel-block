{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wdsbt/wdsbt-carousel-block",
  "version": "0.1.0",
  "title": "WDS-BT Carousel Block",
  "category": "widgets",
  "icon": "slides",
  "description": "A carousel block that accepts other blocks as slides",
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "attributes": {
    "autoPlay": {
      "type": "boolean",
      "default": true
    },
    "autoPlaySpeed": {
      "type": "number",
      "default": 8000
    },
    "showDots": {
      "type": "boolean",
      "default": true
    },
    "showScrollbar": {
      "type": "boolean",
      "default": false
    },
    "buttonStyle": {
      "type": "string",
      "default": "raised",
      "enum": [
        "raised",
        "outlined"
      ]
    }
  },
  "textdomain": "wdsbt-carousel-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}